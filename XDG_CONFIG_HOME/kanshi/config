profile dock {
  output "Sharp Corporation 0x14FA Unknown" disable
  output "GIGA-BYTE TECHNOLOGY CO., LTD. M28U 22170B000449" enable
  exec '. "$XDG_CONFIG_HOME/kanshi/dock.sh"'
}

profile dock {
  output DSI-1 disable
  output "GIGA-BYTE TECHNOLOGY CO., LTD. M28U 22170B000449" enable
  exec '. "$XDG_CONFIG_HOME/kanshi/dock.sh"'
}

profile mobile {
  output "Sharp Corporation 0x14FA Unknown" enable
  exec '. "$XDG_CONFIG_HOME/kanshi/mobile.sh"'
}

profile mobile {
  output DSI-1 enable
  exec '. "$XDG_CONFIG_HOME/kanshi/mobile.sh"'
}
