[calendars]

[[eforah_local]]
path = $XDG_DATA_HOME/cal/eforah/*
type = discover

[[personal_local]]
path = $XDG_DATA_HOME/cal/personal/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
weeknumbers = left
