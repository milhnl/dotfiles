[calendars]

[[gmail_local]]
path = $XDG_DATA_HOME/cal/gmail/*
type = discover

[[milhnl_local]]
path = $XDG_DATA_HOME/cal/milhnl/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
weeknumbers = left
